<Application x:Class="AppleOs.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AppleOs"
             xmlns:test="clr-namespace:AppleOs.Test"
             StartupUri="Test/MainTestWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionaries/GlobalVariables.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Colors.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Buttons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
